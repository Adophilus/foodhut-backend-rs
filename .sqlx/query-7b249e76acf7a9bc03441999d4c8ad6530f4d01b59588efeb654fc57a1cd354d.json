  "db_name": "PostgreSQL",
  "query": "\n            WITH order_items AS (\n                SELECT * FROM order_items\n                WHERE\n                    order_id = $1\n            )\n            SELECT orders.*, JSON_AGG(order_items.*) as items\n                FROM orders JOIN order_items ON order_items.order_id = orders.id\n            WHERE\n                orders.owner_id = $2\n            GROUP BY\n                orders.id;\n        ",
